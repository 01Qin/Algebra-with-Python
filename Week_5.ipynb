        "x1 = 1\n",
        "y1 = 7\n",
        "x2 = 2\n",
        "y2 = 10\n",
        "\n",
        "slope = (y2 - y1) / (x2 - x1)\n",
        "print(slope)\n",
        "\n",
        "intercept = y1 - (slope * x1)\n",
        "print(intercept)"
