      "source": [
        "import sympy\n",
        "from sympy import symbols\n",
        "from sympy.solvers import solve\n",
        "\n",
        "x = symbols('x')\n",
        "y = symbols('y')\n",
        "\n",
        "#put the equation here\n",
        "# 2x = 4\n",
        "eq1 = 2*x - 4\n",
        "\n",
        "\n",
        "eq2 = y**2 - 3 - 22\n",
        "\n",
        "\n",
        "print(\"x = \", solve(eq1, x))\n",
        "print(\"y = \", solve(eq2, y))"
      ]
